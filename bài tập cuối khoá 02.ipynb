    "#    pass\n",
    "\n",
    "def clbtimport():\n",
    "    Manager_dict['type'] = imptype_entry.get()\n",
    "    Manager_dict['impQuantity'] = float(impQua_entry.get())\n",
    "    Manager_dict['imptime'] = imptime_entry.get()\n",
    "    Manager_dict['expiry'] = exp_entry.get()\n",
    "\n",
    "def clbtexport():\n",
    "    Manager_dict['type'] = exptype_entry.get()\n",
    "    Manager_dict['expQuantity'] = float(expQua_entry.get())\n",
    "    Manager_dict['exptime'] = exptime_entry.get()\n",
    "    inventory_label1 = Manager_dict['impQuantity'] - Manager_dict['expQuantity']\n",
